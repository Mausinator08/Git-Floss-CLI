{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program Help",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\cli.js",
      "args": [
        "--help"
      ],
      "preLaunchTask": "post-install"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program commit-current-branch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\cli.js",
      "args": [
        "push-current-branch"
      ],
      "preLaunchTask": "post-install"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Post-Install",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\post-install.js"
    }
  ]
}